		<servlet-class>servlets.Recovery</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Recovery</servlet-name>
		<url-pattern>/recovery</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Validate</display-name>
		<servlet-name>Validate</servlet-name>
		<servlet-class>servlets.Validate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Validate</servlet-name>
